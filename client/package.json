{
	"name": "client",
	"description": "Front-end UI for live chat.",
	"version": "2.0.0",
	"type": "module",
	"scripts": {
		"clean": "rm -r -f dist *.tsbuildinfo",
		"lint": "npx eslint source --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"build": "npm run lint && npx tsc && npx vite build",
		"dev": "npx vite",
		"preview": "npm run clean && npm run build && npx vite preview",
		"test": "npm run clean && npm run build && NODE_ENV=test npx vitest run"
	},
	"private": true,
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.18.0"
	},
	"devDependencies": {
		"@types/react": "^18.2.37",
		"@types/react-dom": "^18.2.15",
		"@vitejs/plugin-react": "^4.1.1",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.53.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.4",
		"postcss": "^8.4.31",
		"tailwindcss": "^3.3.5",
		"vite": "^4.5.0",
		"vite-plugin-checker": "^0.6.2",
		"vitest": "^0.34.6"
	},
	"engines": {
		"node": ">=20.7.0",
		"npm": ">=10.1.0"
	}
}
